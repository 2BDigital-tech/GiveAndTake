<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.giveandtake">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.android.giveandtake.Admin.PostAdmin"></activity>
        <activity android:name="com.android.giveandtake.home.Post_activity" />
        <activity android:name="com.android.giveandtake.Admin.usertest" />
        <activity android:name="com.android.giveandtake.Admin.ShowUsers" />
        <activity android:name="com.android.giveandtake.Admin.AdminConnect" />
        <activity android:name=".ProfileActivity" />
        <activity
            android:name="com.android.giveandtake.Connect_Fragment"
            android:label="@string/title_activity_main2" />
        <activity android:name="com.android.giveandtake.LoginActivity" />
        <activity android:name="com.android.giveandtake.RegistrationActivity" />
        <activity android:name="com.android.giveandtake.Start_Application">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>